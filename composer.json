{
    "name": "acseo/ai-phpunit",
    "description": "Writes missing PHPUnit tests using GPT-4.",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "ACSEO\\AIPHPUnit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ACSEO",
            "email": "contact@acseo.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/console": "^6",
        "openai-php/client": "^0.10.0",
        "symfony/http-client": "^7.1",
        "nyholm/psr7": "^1.8"
    },
    "bin": [
        "bin/aiphpunit"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
